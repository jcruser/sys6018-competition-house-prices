summary(p2.4b)
p2.4c <- (summary(p2.4a)$r.squared)
p2.4c
p2.4d <- predict(p2.4a, newdata=list(x1=275), interval="confidence", level=0.95)
p2.4d
p2.4e <- predict(p2.4a, newdata=list(x1=275), interval="predict", level=0.95)
p2.4e
