B1 <- read_excel("data-table-B1.xls") #Read in the xls data file
B3 <- read_excel("data-table-B3.xls") #Read in the xls data file
p2.1a <- lm(y ~ x8, data=B1)
p2.1a
p2.1b <- aov(y ~ x8, data=B1) #Find the analysis of variance
summary(p2.1b) #Construct ANOVA table
p2.1c <- confint(p2.1a, 'x8', level=0.95)
p2.1c
p2.1d <- (summary(p2.1a)$r.squared)
p2.1d
B1.1 = data.frame(x8=2000) #Set opponents' yards rushed to 2000
p2.1e <- predict(p2.1a, B1.1, interval="predict")
p2.1e
p2.1e <- predict(p2.1a, newdata=B1.1, interval="predict")
p2.1e
p2.1e <- predict(p2.1a, newdata=B1.1, interval="predict", level=.95)
p2.1e
View(B1.1)
p2.1e <- predict(p2.1a, newdata=list(x8=2000), interval="predict", level=0.95)
p2.1e
p2.1e <- predict(p2.1a, newdata=list(x8=2000), interval="confidence", level=0.95)
p2.1e
p2.2 <- predict(p2.1a, newdata=list(x8=1800), interval="confidence", level=0.90)
p2.2
p2.2 <- predict(p2.1a, newdata=list(x8=1800), interval="predict", level=0.90)
p2.2
